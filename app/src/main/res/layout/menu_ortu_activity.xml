<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.eladoktarizo.monitoring_belajar.MenuOrtu">

    <include
        android:id="@+id/toolbar_menuortu"
        layout="@layout/toolbar_layout" />

    <Button
        android:id="@+id/dataanakortu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data Anak"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="112dp"
        android:layout_above="@+id/tentang"
        android:layout_toStartOf="@+id/logout"
        android:layout_marginBottom="108dp" />

    <Button
        android:id="@+id/monitoring"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Monitoring"
        tools:layout_editor_absoluteX="220dp"
        tools:layout_editor_absoluteY="112dp"
        android:layout_alignBaseline="@+id/tentang"
        android:layout_alignBottom="@+id/tentang"
        android:layout_alignEnd="@+id/profilguru" />

    <Button
        android:id="@+id/profilguru"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profil Guru"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="231dp"
        android:layout_alignBaseline="@+id/dataanakortu"
        android:layout_alignBottom="@+id/dataanakortu"
        android:layout_toEndOf="@+id/logout" />

    <Button
        android:id="@+id/tentang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tentang Aplikasi"
        tools:layout_editor_absoluteX="220dp"
        tools:layout_editor_absoluteY="230dp"
        android:layout_above="@+id/logout"
        android:layout_alignParentStart="true"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="46dp" />

    <Button
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Logout"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="317dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="81dp" />
</RelativeLayout>
